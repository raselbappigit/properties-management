<div id="user-list">
    <h2>
        User List</h2>
    <p>
        @Html.ActionLink("Create User", "Create", "User")
        | @Html.ActionLink("Manage Role", "Index", "Role")
    </p>
    <div id="users-data-table">
        <table id="userDataTable" class="dataTable">
            <thead>
                <tr>
                    <th>
                    </th>
                    <th>
                        Name
                    </th>
                    <th>
                        Email
                    </th>
                    <th>
                        IsApproved
                    </th>
                    <th>
                        Created
                    </th>
                    <th>
                        Last Login
                    </th>
                    <th>
                        Last Activity
                    </th>
                    <th>
                        Last Password Change
                    </th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
<script src="../../Scripts/App/user-data-tables.js" type="text/javascript"></script>
