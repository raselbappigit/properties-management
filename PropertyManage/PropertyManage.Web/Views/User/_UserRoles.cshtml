@model IEnumerable<PropertyManage.Web.AppPrivilegeModel>
<div id="user-role-list">
    @if (Model != null && Model.Count() != 0)
    {

        foreach (var item in Model.ToList())
        {
        <div class="display-field">
            <strong>Privilege: </strong><span>@Html.DisplayFor(model => item.PrivilegeName)</span>
        </div>

        }
    }
    else
    {
        <div class="display-field">
            <strong>Nothing found - Sorry</strong></div> 
    }
</div>
