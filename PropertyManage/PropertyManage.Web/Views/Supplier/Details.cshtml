        <div class="box-body">
            <div class="box-header clear">
                <h2>
                    Details Supplier
                </h2>
            </div>
            <div class="box-wrap clear">
                <div class="form bt-space15">
                    <div class="columns clear bt-space15">
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.Name)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.Name)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.Address)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.Address)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.Phone)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.Phone)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.Mobile)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.Mobile)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.Fax)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.Fax)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.Email)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.Email)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.ContactPerson)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.ContactPerson)
                            </div>
                        </div>
                        <div class="form-field clear">
                            <div class="form-label size-120 fl-space2">
                                @Html.LabelFor(m => m.ContactDesignation)
                            </div>
                            <div class="text fl-space2">
                                @Html.DisplayFor(m => m.ContactDesignation)
